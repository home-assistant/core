        }
      },
      "name": "Set air cleaner mode"
    },
    "set_dehumidify_setpoint": {
      "description": "Sets the target humidity for cooling.",
      "fields": {
        "humidity": {
          "description": "The setpoint for dehumidification when cooling.",
