{
  "config": {
    "step": {
      "user": {
        "title": "Sign-in with Ring account",
        "data": {
          "username": "[%key:common::config_flow::data::username%]",
          "password": "[%key:common::config_flow::data::password%]"
        }
      },
      "2fa": {
        "title": "Two-factor authentication",
        "data": {
          "2fa": "Two-factor code"
