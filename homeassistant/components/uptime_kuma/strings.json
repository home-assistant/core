        "description": "Set up **Uptime Kuma** monitoring service",
        "data": {
          "url": "[%key:common::config_flow::data::url%]",
          "verify_ssl": "[%key:common::config_flow::data::verify_ssl%]",
          "api_key": "[%key:common::config_flow::data::api_key%]"
