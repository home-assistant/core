{
  "config": {
    "flow_title": "Homee {name} ({host})",
    "abort": {
      "already_configured": "[%key:common::config_flow::abort::already_configured_device%]"
    },
    "error": {
      "cannot_connect": "[%key:common::config_flow::error::cannot_connect%]",
      "invalid_auth": "[%key:common::config_flow::error::invalid_auth%]",
      "unknown": "[%key:common::config_flow::error::unknown%]"
    },
    "step": {
      "user": {
        "title": "Configure homee",
        "data": {
          "host": "[%key:common::config_flow::data::host%]",
          "password": "[%key:common::config_flow::data::password%]",
          "username": "[%key:common::config_flow::data::username%]"
        },
        "data_description": {
          "host": "The IP address of your Homee.",
          "username": "The username for your Homee.",
          "password": "The password for your Homee."
        }
      }
    }
  },
  "entity": {
    "sensor": {
      "battery_sensor": {
        "name": "[%key:component::sensor::entity_component::battery::name%]"
      },
      "brightness_sensor": {
        "name": "[%key:component::sensor::entity_component::illuminance::name%]"
      },
      "brightness_sensor_instance": {
        "name": "Illuminance {instance}"
      },
      "button_state_sensor": {
        "name": "Button State"
      },
      "button_state_sensor_instance": {
        "name": "Button State {instance}"
      },
      "current_sensor": {
        "name": "[%key:component::sensor::entity_component::current::name%]"
      },
      "current_sensor_instance": {
        "name": "Current instance"
      },
      "dawn_sensor": {
        "name": "Dawn"
      },
      "device_temperature_sensor": {
        "name": "Device Temperature"
      },
      "energy_sensor": {
        "name": "[%key:component::sensor::entity_component::energy::name%]"
      },
      "energy_sensor_instance": {
        "name": "Energy {instance}"
      },
      "relative_humidity_sensor": {
        "name": "Relative Humidity"
      },
      "link_quality_sensor": {
        "name": "Link Quality"
      },
      "node_sensor_protocol": {
        "name": "Node Protocol"
      },
      "node_sensor_state": {
        "name": "Node State"
      },
      "position_sensor": {
        "name": "Position"
      },
      "power_sensor": {
        "name": "[%key:component::sensor::entity_component::power::name%]"
      },
      "power_sensor_instance": {
        "name": "Power {instance}"
      },
      "rainfall_hour_sensor": {
        "name": "Rainfall Last Hour"
      },
      "rainfall_day_sensor": {
        "name": "Rainfall Today"
      },
      "temperature_sensor": {
        "name": "[%key:component::sensor::entity_component::temperature::name%]"
      },
      "total_current_sensor": {
        "name": "Total Current"
      },
      "total_energy_sensor": {
        "name": "Total Energy"
      },
      "total_power_sensor": {
        "name": "Total Power"
      },
      "total_voltage_sensor": {
        "name": "Total Voltage"
      },
      "up_down_sensor": {
        "name": "State",
        "state": {
          "open": "Open",
          "closed": "Closed",
          "partial": "Partially Open",
          "opening": "Opening",
          "closing": "Closing"
        }
      },
      "uv_sensor": {
        "name": "Ultraviolet"
      },
      "valve_position_sensor": {
        "name": "Valve Position"
      },
      "voltage_sensor": {
        "name": "[%key:component::sensor::entity_component::voltage::name%]"
      },
      "voltage_sensor_instance": {
        "name": "Voltage {instance}"
      },
      "wind_speed_sensor": {
        "name": "[%key:component::sensor::entity_component::wind_speed::name%]"
      },
      "window_position_sensor": {
        "name": "Window Position",
        "state": {
          "0": "Closed",
          "1": "Open",
          "2": "Tilted"
        }
      }
    }
  }
}
