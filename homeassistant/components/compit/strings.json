      },
      "co2_level": {
        "name": "CO2 level"
      },
      "dust_alert": {
        "name": "Dust alert"
      },
      "has_battery": {
        "name": "Has battery"
      },
      "has_external_power": {
        "name": "Has external power"
      },
      "pump_status": {
        "name": "Pump status"
