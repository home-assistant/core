{
  "domain": "lamarzocco",
  "name": "La Marzocco",
  "bluetooth": [
    {
      "local_name": "MICRA_*"
    },
    {
      "local_name": "MINI_*"
    },
    {
      "local_name": "GS3_*"
    },
    {
      "local_name": "GS3AV_*"
    }
  ],
  "codeowners": ["@zweckj"],
  "config_flow": true,
  "dependencies": ["bluetooth_adapters"],
  "dhcp": [
    {
      "registered_devices": true
    },
    {
      "hostname": "gs[0-9][0-9][0-9][0-9][0-9][0-9]"
    },
    {
      "hostname": "lm[0-9][0-9][0-9][0-9][0-9][0-9]"
    },
    {
      "hostname": "mr[0-9][0-9][0-9][0-9][0-9][0-9]"
    }
  ],
  "documentation": "https://www.home-assistant.io/integrations/lamarzocco",
  "integration_type": "device",
  "iot_class": "cloud_polling",
  "loggers": ["pylamarzocco"],
  "requirements": ["pylamarzocco==1.2.12"]
}
