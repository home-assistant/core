      "nightlight": {
        "name": "Nightlight"
      },
      "reverse": {
        "name": "Reverse"
      },
      "segment_reverse": {
        "name": "Segment {segment} reverse"
      },
      "sync_receive": {
        "name": "Sync receive"
      },
      "sync_send": {
        "name": "Sync send"
      }
