      "info_leds_max_power": {
        "name": "Max current"
      },
      "ip": {
        "name": "IP"
      },
      "uptime": {
        "name": "Uptime"
      },
      "wifi_bssid": {
        "name": "Wi-Fi BSSID"
      },
      "wifi_channel": {
        "name": "Wi-Fi channel"
      },
      "wifi_rssi": {
        "name": "Wi-Fi RSSI"
      },
      "wifi_signal": {
        "name": "Wi-Fi signal"
      }
