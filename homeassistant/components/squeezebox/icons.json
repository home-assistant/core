{
  "entity": {
    "sensor": {
      "info_total_albums": {
        "default": "mdi:album"
      },
      "info_total_artists": {
        "default": "mdi:account-music"
      },
      "info_total_genres": {
        "default": "mdi:drama-masks"
      },
      "info_total_songs": {
        "default": "mdi:file-music"
      },
      "player_count": {
        "default": "mdi:folder-play"
      },
      "other_player_count": {
        "default": "mdi:folder-play-outline"
      }
    },
    "switch": {
      "alarms_enabled": {
        "default": "mdi:alarm-check",
        "state": {
          "on": "mdi:alarm-check",
          "off": "mdi:alarm-off"
        }
      },
      "alarm": {
        "default": "mdi:alarm",
        "state": {
          "on": "mdi:alarm",
          "off": "mdi:alarm-off"
        }
      }
    }
  },
  "services": {
    "call_method": {
      "service": "mdi:console"
    },
    "call_query": {
      "service": "mdi:database"
    }
  }
}
