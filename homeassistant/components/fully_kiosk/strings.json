      "internal_storage_total_space": {
        "name": "Internal storage total space"
      },
      "internal_storage_free_space": {
        "name": "Internal storage free space"
      },
      "ram_free_memory": {
        "name": "Free memory"
      },
      "ram_total_memory": {
        "name": "Total memory"
      }
